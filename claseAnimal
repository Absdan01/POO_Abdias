class Animal:
    def __init__(self, nombre):
        self.nombre = nombre
    
    def hablar(self): 
        pass 

class Perro(Animal):
    def hablar(self): 
        return "Guau!"

class Gato(Animal):
    def hablar(self): 
        return "Miau!"

# Uso de la jerarqu√≠a Animal
perro = Perro("Firulais")
print(f"El perro dice: {perro.hablar()}")
print("-" * 20)
