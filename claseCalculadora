class Calculadora:
    def sumar(self, a, b): 
        return a + b
    
    def restar(self, a, b):
        return a - b 
    
    def multiplicar(self, a, b): 
        return a * b
    
    def dividir(self, a, b):
        if b == 0:
            return "Error: divisi√≥n entre cero"
        return a / b

# Uso de la clase Calculadora
calc = Calculadora()
print(f"Suma (5+3): {calc.sumar(5, 3)}")
print("-" * 20)
